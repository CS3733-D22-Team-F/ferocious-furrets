<AnchorPane fx:id="masterPane" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../RequestPages.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D22.teamF.controllers.requests.scanController">
   <children>
      <ImageView fitHeight="732.0" fitWidth="1295.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/CTScan image.jpeg" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#123090" height="730.0" stroke="BLACK" strokeType="INSIDE" width="574.0" />
      <Pane layoutX="-11.0" layoutY="-4.0" prefHeight="115.0" prefWidth="585.0" style="-fx-background-color: #27aad0;">
         <children>
            <Label contentDisplay="TOP" layoutX="78.0" layoutY="44.0" prefHeight="71.0" prefWidth="382.0" text="Scan Requests" textFill="WHITE">
               <font>
                  <Font name="Serif Regular" size="64.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
            <ImageView fitHeight="64.0" fitWidth="70.0" layoutX="476.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../BWHlogo-new.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <HBox layoutX="90.0" layoutY="505.0" prefHeight="53.0" prefWidth="374.0">
         <children>
            <JFXButton buttonType="RAISED" onAction="#submit" prefHeight="85.0" prefWidth="174.0" ripplerFill="#f2ba49" text="Submit" textFill="WHITE">
               <font>
                  <Font name="Serif Regular" size="24.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="reset" buttonType="RAISED" onAction="#reset" prefHeight="53.0" prefWidth="161.0" ripplerFill="#f3ba48" text="Reset" textFill="WHITE">
               <font>
                  <Font name="Serif Regular" size="24.0" />
               </font>
            </JFXButton>
            <JFXButton buttonType="RAISED" onAction="#switchToHome" prefHeight="53.0" prefWidth="159.0" ripplerFill="#f3ba48" text="Return" textFill="WHITE">
               <font>
                  <Font name="Serif Regular" size="24.0" />
               </font>
            </JFXButton>
         </children>
      </HBox>
      <VBox layoutX="282.0" layoutY="182.0" prefHeight="308.0" prefWidth="261.0">
