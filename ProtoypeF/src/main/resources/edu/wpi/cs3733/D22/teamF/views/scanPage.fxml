            <JFXComboBox fx:id="nodeField" prefHeight="50.0" prefWidth="318.0" promptText="Location">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXComboBox>
            <JFXComboBox fx:id="employeeIDField" prefHeight="50.0" prefWidth="318.0" promptText="Assigned Employee">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXComboBox>
            <JFXComboBox fx:id="userField" prefHeight="50.0" prefWidth="318.0" promptText="Request Employee">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXComboBox>
            <JFXComboBox fx:id="typeChoice" focusColor="WHITE" prefHeight="41.0" prefWidth="348.0" promptText="Scan Type" unFocusColor="WHITE">
               <VBox.margin>
                  <Insets bottom="10.0" top="5.0" />
               </VBox.margin>
            </JFXComboBox>
            <JFXComboBox fx:id="statusChoice" focusColor="WHITE" prefHeight="46.0" prefWidth="337.0" promptText="Status" unFocusColor="WHITE">
               <VBox.margin>
                  <Insets bottom="10.0" top="5.0" />
               </VBox.margin>
            </JFXComboBox>
         </children>
      </VBox>
      <VBox layoutX="17.0" layoutY="185.0" prefHeight="316.0" prefWidth="242.0" spacing="22.0">
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="33.0" prefWidth="306.0" text="Location:" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <Label prefHeight="33.0" prefWidth="306.0" text="Assigned Employee:" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="33.0" prefWidth="306.0" text="Request Employee:" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="33.0" prefWidth="306.0" text="Scan Type:" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="33.0" prefWidth="306.0" text="Status:" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <HBox layoutX="118.0" layoutY="586.0">
         <children>
            <TextField fx:id="reqID" prefHeight="43.0" prefWidth="194.0" promptText="Request ID">
               <font>
                  <Font name="Serif Regular" size="24.0" />
               </font></TextField>
            <Button fx:id="resolveReq" mnemonicParsing="false" onAction="#resolveRequest" prefHeight="43.0" prefWidth="128.0" style="-fx-background-color: #012090;" text="Resolve" textFill="WHITE">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
               <font>
                  <Font name="Serif Regular" size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
