               <children>
                  <Label contentDisplay="TOP" prefHeight="100.0" prefWidth="536.0" text="Patient Delivery" textFill="WHITE">
                     <font>
                        <Font name="Serif Regular" size="64.0" />
                     </font>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </Label>
                  <ImageView fitHeight="100.0" fitWidth="75.0" pickOnBounds="true" />
               </children>
            </HBox>
            <HBox fx:id="bottomHBox" layoutX="120.0" layoutY="575.0" prefHeight="53.0" prefWidth="300.0">
               <children>
                  <JFXButton fx:id="submitButton" buttonType="RAISED" onAction="#submit" prefHeight="53.0" prefWidth="175.0" ripplerFill="#f3ba48" text="Submit" textFill="WHITE">
                     <font>
                        <Font size="28.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="resetButton" buttonType="RAISED" onAction="#reset" prefHeight="53.0" prefWidth="175.0" ripplerFill="#f3ba48" text="Reset" textFill="WHITE">
                     <font>
                        <Font size="28.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
            <HBox fx:id="middleHBox" layoutX="2.0" layoutY="149.0" prefHeight="363.0" prefWidth="588.0">
               <children>
                  <VBox alignment="TOP_RIGHT" prefHeight="401.0" prefWidth="288.0" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="130.0" text="Location: " textFill="WHITE">
                           <font>
                              <Font name="Serif Regular" size="24.0" />
