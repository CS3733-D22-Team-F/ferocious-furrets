                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Label>
                     </children>
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </VBox>
                  <VBox prefHeight="401.0" prefWidth="288.0" spacing="10.0">
                     <children>
                        <JFXComboBox fx:id="nodeField" prefHeight="50.0" prefWidth="287.0" promptText=" Location">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </JFXComboBox>
                        <JFXComboBox fx:id="employeeIDField" prefHeight="50.0" prefWidth="531.0" promptText=" Assigned Employee">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </JFXComboBox>
                        <JFXComboBox fx:id="userField" prefHeight="50.0" prefWidth="531.0" promptText=" Requested Employee">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </JFXComboBox>
                        <TextField fx:id="addressField" prefHeight="50.0">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </TextField>
                        <JFXComboBox fx:id="methodField" prefHeight="50.0" prefWidth="541.0" promptText=" Method">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </JFXComboBox>
                        <JFXComboBox fx:id="statusChoice" prefHeight="50.0" prefWidth="608.0" promptText=" Status">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </JFXComboBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </Pane>
   </left>
   <center>
      <Pane fx:id="tablePane" prefHeight="720.0" prefWidth="536.0" BorderPane.alignment="CENTER">
         <children>
            <JFXTreeTableView fx:id="treeTable" prefHeight="720.0" prefWidth="640.0" />
         </children>
      </Pane>
   </center>
</BorderPane>
