<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="500.0" maxWidth="1000.0" minHeight="500.0" minWidth="1000.0" prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D22.teamF.Map.MapEquipModifyController">
   <children>
      <AnchorPane fx:id="iconPane" prefHeight="500.0" prefWidth="700.0">
         <children>
            <ImageView fx:id="mapHolder" fitHeight="500.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
      <VBox layoutX="725.0" layoutY="-3.0" prefHeight="509.0" prefWidth="260.0">
         <children>
            <Label prefHeight="35.0" prefWidth="156.0" text="Current: ">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Label fx:id="currentNode" prefHeight="23.0" prefWidth="392.0">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Label>
            <Label prefHeight="35.0" prefWidth="156.0" text="Equipment:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <JFXComboBox fx:id="nodeBox" onAction="#onRoomChange" prefHeight="26.0" prefWidth="289.0" />
            <Label prefHeight="35.0" prefWidth="156.0" text="Status:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="statusLabel" prefHeight="23.0" prefWidth="392.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <HBox prefHeight="39.0" prefWidth="260.0">
               <children>
                  <JFXButton fx:id="cleanButton" disable="true" onAction="#toClean" prefHeight="46.0" prefWidth="130.0" text="Mark Clean" underline="true" />
                  <JFXButton fx:id="dirtyButton" disable="true" onAction="#toDirty" prefHeight="64.0" prefWidth="130.0" text="Mark Dirty" underline="true" />
               </children>
            </HBox>
            <JFXButton fx:id="cancel" onAction="#cancel" prefHeight="49.0" prefWidth="122.0" ripplerFill="RED" text="Cancel" />
            <JFXButton onAction="#submit" prefHeight="49.0" prefWidth="122.0" ripplerFill="#182c9e" text="Submit" />
            <JFXButton onAction="#delete" prefHeight="49.0" prefWidth="122.0" ripplerFill="#182c9e" text="Delete" />
            <JFXButton onAction="#reset" prefHeight="49.0" prefWidth="122.0" ripplerFill="#182c9e" text="Reset" />
         </children>
      </VBox>
   </children>
</AnchorPane>
